<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/navigation_new_print_request"
            app:startDestination="@id/actionNewRequest">


    <fragment
            android:id="@+id/actionNewRequest"
            android:name="com.ng.printtag.printrequest.FragmentNewPrintRequest"
            tools:layout="@layout/fragment_new_print_request">


        <action
                android:id="@+id/actionAddProducts"
                app:destination="@id/actionAddProducts"
                app:exitAnim="@anim/push_out_to_right"
                app:popEnterAnim="@anim/push_in_from_right"
                app:enterAnim="@anim/push_in_from_left"
                app:popExitAnim="@anim/push_out_to_left"/>
    </fragment>
    <fragment
            android:id="@+id/actionAddProducts"
            android:name="com.ng.printtag.printrequest.FragmentAddProduct"
            tools:layout="@layout/fragment_add_product">
    </fragment>
</navigation>